@section scripts
{
    <script type="text/javascript">
        $(document).ready(function() {



            $("#getl").on("click", function () {
                ga('send', 'event', 'adjusted bounce rate', 'user clicked convert');
                });


            $('#qrcode').qrcode({
                width: 110, height: 110,
                text: "http://vidfrom.com/"
                });
            $('#navbtn').on('click', function () {
                if ($("#navbar").hasClass("in")) {
                    $(".mcon").animate({
                        'padding-top': '0'
                    });
                    //$(".mcon").css("padding-top", "0");
                } else {
                    //$(".mcon").css("padding-top", "10%");
                    $(".mcon").animate({
                        'padding-top': '20%'
                    });
                }
                // Grab height of dropdown
                //var height = $('.dropdown').outerHeight();
            });


            $("#dwnmp").on("click", function() {
                $('#res').empty();
                $("#resaultP").css("display", "none");
                $("#err").css("display", "none");
                $("#load").css("display", "block");

                var tit = $("#lnktit").text().trim();
                var urld = $("#url").val().trim();
                //var urld = escape($("#url").val().trim());
                $.ajax({
                        type: "GET",
                        url: "@Url.Action("GetMp3", "Home")",
                        data: ({ url: urld, title: tit })

                    })
                    .success(function(data) {
                        if (data === null || typeof data === "undefined" || data.indexOf("/tb/") === -1) {
                            var er = $("#err");
                            er.html("<b>Error:</b> Time out Please try again.");
                            $("#load").css("display", "none");
                            er.css("display", "block");
                            er.addClass("alert alert-danger");
                            $("#det").css("display", "none");
                            return;
                        }

                        var n = data.replace("/tb/", "");
                        $("#n").val(n);
                        $("#load").css("display", "none");
                        $("#mp").css("display", "none");
                        $("#auziodiv").css("display", "block");
                        var estit = escape(tit);
                        $('#qrcode').qrcode({
                            width: 110,
                            height: 110,
                            text: "http://vidfrom.com/mp3/?n=" + n + "&t=" + estit
                        });
                        return;
                    })
                    .error(function() {
                        var er = $("#err");
                        er.html("<b>Error:</b> Time out Please try again.");
                        $("#load").css("display", "none");
                        er.css("display", "block");
                        er.addClass("alert alert-danger");
                        $("#det").css("display", "none");
                    });

                $("#t").val(tit);

            });

            $(".lead a").on("click", function() {
                return false;
            });
            $(".wel a").on("click", function() {
                return false;
            });

        });

        $('#bookmark-this').click(function(e) {
            var bookmarkURL = window.location.href;
            var bookmarkTitle = document.title;

            if ('addToHomescreen' in window && window.addToHomescreen.isCompatible) {
                // Mobile browsers
                addToHomescreen({ autostart: false, startDelay: 0 }).show(true);
            } else if (window.sidebar && window.sidebar.addPanel) {
                // Firefox version < 23
                window.sidebar.addPanel(bookmarkTitle, bookmarkURL, '');
            } else if ((window.sidebar && /Firefox/i.test(navigator.userAgent)) || (window.opera && window.print)) {
                // Firefox version >= 23 and Opera Hotlist
                $(this).attr({
                    href: bookmarkURL,
                    title: bookmarkTitle,
                    rel: 'sidebar'
                }).off(e);
                return true;
            } else if (window.external && ('AddFavorite' in window.external)) {
                // IE Favorite
                window.external.AddFavorite(bookmarkURL, bookmarkTitle);
            } else {
                // Other browsers (mainly WebKit - Chrome/Safari)
                alert('Press ' + (/Mac/i.test(navigator.userAgent) ? 'Cmd' : 'Ctrl') + '+D to bookmark this page.');
            }

            return false;
        });

    </script>
    <script>
        (function(i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function() {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-77340743-1', 'auto');
        ga('send', 'pageview');

    </script>
